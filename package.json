{
  "name": "ts-rename-all",
  "private": true,
  "description": "Once rename a file name, then symbols are also renamed.",
  "version": "0.11.5",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm turbo build",
    "lint": "pnpm turbo lint",
    "typecheck": "pnpm turbo typecheck",
    "test:ci": "pnpm turbo test:ci",
    "bump": "bumpp -r"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bumpp": "^9.7.1",
    "changelogithub": "^0.13.11",
    "eslint": "^8.57.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "turbo": "^1.13.4",
    "typescript": "^5.6.3"
  },
  "prettier": {
    "singleQuote": true
  },
  "config": {
    "commitizen": {
      "types": [
        {
          "value": "feat",
          "name": "feat:     New feature"
        },
        {
          "value": "fix",
          "name": "fix:      Bug fix"
        },
        {
          "value": "improve",
          "name": "improve:  Improves feature or performance"
        },
        {
          "value": "docs",
          "name": "docs:     Documentation only changes"
        },
        {
          "value": "refactor",
          "name": "refactor: Code refactoring"
        },
        {
          "value": "chore",
          "name": "chore:    Other changes that don't modify src or test files"
        }
      ],
      "skipQuestions": [
        "scope",
        "body",
        "footerPrefix",
        "footer"
      ]
    }
  }
}
