{
  "name": "@ts-rename-all/cli",
  "description": "Once rename, then others are also renamed the same way.",
  "version": "0.10.0",
  "license": "MIT",
  "keywords": [
    "cli",
    "refactor",
    "typescript"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "ts-rename-all": "./dist/cli.js"
  },
  "engines": {
