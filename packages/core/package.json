{
  "name": "@ts-rename-all/core",
  "description": "Once rename, then others are also renamed the same way.",
  "version": "0.5.1",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage.enabled --coverage.provider=v8 --coverage.all --coverage.clean",
    "test:ci": "pnpm run test:coverage --run"
  },
  "dependencies": {
    "@ts-rename-all/shared": "workspace:*",
    "change-case": "^4.1.2",
    "just-intersect": "^4.3.0",
    "just-remove": "^3.2.0",
    "ts-morph": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.7",
    "@vitest/coverage-v8": "^0.34.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.5",
    "vitest": "^0.34.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bisquit/ts-rename-all.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/bisquit/ts-rename-all/issues"
  }
}
