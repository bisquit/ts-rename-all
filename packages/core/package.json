    "@types/node": "^20.4.7",
    "tsup": "^7.1.0",
    "typescript": "^5.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bisquit/ts-rename-all.git",
    "directory": "packages/core"
